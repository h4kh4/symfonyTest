<body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
		<a class="navbar-brand" href=" {{ path('homepage') }}">H4KH4</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor02"
			aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse" id="navbarColor02">
			<ul class="navbar-nav mr-auto">
				<li class="nav-item">
					<a class="nav-link" href=" {{ path('homepage')}}">Accueil</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href=" {{ path('ad')}}">Tracklist</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href=" {{ path('fan')}}">Fan Zone</a>
				</li>
			</ul>
			<ul class="navbar-nav ml-auto">
				{% if app.user %}

					<li class="nav-item">
						<a href="{{ path ('account_profile')}}" class="nav-link">Edit</a>
					</li>
				
					<li class="nav-item">
					<a href="{{ path ('account_logout')}}" class="nav-link">Logout</a>
					</li>
			
				{% else %}
					<li class="nav-item">
						<a href=" {{ path('account_login')}} " class="nav-link">Mon compte</a>
					</li>

					<li class="nav-item">
						<a href="{{ path ('account_register')}}" class="nav-link">Inscription</a>
					</li>
					
				{% endif %}

			
			</ul>
		</div>
	</nav>

	{#========== Message d'alertes ===========#}

	{% for label, messages in app.flashes %}
	<div class="container mt-4">
		<div class="alert alert-{{ label }}">
			{% for message in messages %}
			<p> {{ message | raw }}</p>
			{% endfor %}
		</div>
	</div>
	{% endfor %}
